<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<bean class="com.jspiders.springcore.entity.User" name="user1">
		<property name="id" value="1" />
		<property name="userName" value="Girish" />
		<property name="email" value="girishamrutkar6@gmail.com" />
		<property name="password" value="Girish@1234" />
	</bean>
	<bean class="com.jspiders.springcore.entity.User" name="user2">
		<property name="id" value="2" />
		<property name="userName" value="Rushya" />
		<property name="email" value="khorrushikesh@gmail.com" />
		<property name="password" value="Rushi@1234" />
	</bean>
	<bean class="com.jspiders.springcore.entity.User" name="user3">
		<constructor-arg name="id" value="3" />
		<constructor-arg name="userName" value="Rohit" />
		<constructor-arg name="email" value="rohitpatil@gmail.com" />
		<constructor-arg name="password" value="Rohit@1234" />
	</bean>
</beans>